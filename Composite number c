#include<stdio.h>
int main ()
{
	int i,j,n,a[100],count=0;
	printf("Enter size:");
	scanf("%d",&n);
	printf("enter elements\n");
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
}
for(i=0;i<n;i++){
	for (j=2;j<a[i];j++)
	{
		if (a[i]%j==0)
		{
			printf("%d ",a[i]);
			count++;
			
			break;
		}
	}

}
printf("total composite number are:%d",count);
}
